@inject IWorkContext workContext
{
    "emptyTable":     "@JavaScriptEncoder.Default.Encode("EmptyTable")",
    "info":           "@JavaScriptEncoder.Default.Encode("Info")",
    "infoEmpty":      "@JavaScriptEncoder.Default.Encode("InfoEmpty")",
    "infoFiltered":   "@JavaScriptEncoder.Default.Encode("InfoFiltered")",
    "thousands":      "@JavaScriptEncoder.Default.Encode("Thousands")",
    "lengthMenu":     "@JavaScriptEncoder.Default.Encode("LengthMenu")",
    "loadingRecords": "@JavaScriptEncoder.Default.Encode("LoadingRecords")",
    "processing":     "@JavaScriptEncoder.Default.Encode("LoaderIcon")",
    "search":         "@JavaScriptEncoder.Default.Encode("Search")",
    "zeroRecords":    "@JavaScriptEncoder.Default.Encode("ZeroRecords")",
    "paginate": {
        "first":"@JavaScriptEncoder.Default.Encode("First")",
        "last":"@JavaScriptEncoder.Default.Encode("Last")",
        "next":"@JavaScriptEncoder.Default.Encode("Next")",
        "previous":"@JavaScriptEncoder.Default.Encode("previous")"
    },
    "aria": {
        "sortAscending":  "@JavaScriptEncoder.Default.Encode("SortAscending")",
        "sortDescending": "@JavaScriptEncoder.Default.Encode("SortDescending")"
    }
